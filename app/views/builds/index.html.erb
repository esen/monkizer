<p id="notice"><%= notice %></p>

<h2>Listing Builds</h2>

<table class="pure-table">
  <thead>
    <tr>
      <th>Project</th>
      <th>Ci build number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @builds.each do |build| %>
      <tr>
        <td><%= build.project_id %></td>
        <td><%= build.ci_build_number %></td>
        <td><%= link_to 'Show', [@project, build] %></td>
        <td><%= link_to 'Edit', edit_project_build_path(@project, build) %></td>
        <td><%= link_to 'Destroy', [@project, build], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Build', new_project_build_path(@project) %>
